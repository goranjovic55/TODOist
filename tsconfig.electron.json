{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "outDir": "build/electron",
    "rootDir": "src/electron",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
  "include": ["src/electron"],
  "exclude": ["node_modules"]
} 